{
  "name": "vehicle-consultation-api",
  "version": "0.0.1",
  "description": "",
  "license": "ISC",
  "keywords": [
    "clean architecture",
    "clean code",
    "tdd",
    "ddd",
    "solid",
    "design patterns",
    "node",
    "typescript",
    "jest",
    "typeorm"
  ],
  "scripts": {
    "start": "node -r dotenv/config dist/main",
    "dev": "cross-env NODE_ENV=local ts-node-dev --respawn --transpile-only --ignore-watch node_modules --clear -r dotenv/config src/main",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig-build.json",
    "prepare": "husky install",
    "check": "npm-check -s -u",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm t -- --watch",
    "test:staged": "npm t -- --findRelatedTests",
    "test:coverage": "npm t -- --coverage",
    "test:integration": "npm t -- --config ./jest.integration.config.js",
    "test:coveralls": "npm run test:coverage && coveralls < coverage/lcov.info"
  },
  "dependencies": {
    "@redis/client": "^1.5.13",
    "axios": "^0.21.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@jest-mock/express": "^1.4.4",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.7.13",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "coveralls": "^3.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "jest-mock-extended": "^2.0.1",
    "lint-staged": "^11.1.2",
    "mockdate": "^3.0.5",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": "18.x"
  }
}
